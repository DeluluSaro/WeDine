{
  "crons": [
    {
      "path": "/api/cron/cleanup-orders",
      "schedule": "0 0 * * *"
    }
  ],
  "functions": {
    "app/api/cron/cleanup-orders/route.ts": {
      "maxDuration": 300
    },
    "app/api/orders/cleanup/route.ts": {
      "maxDuration": 300
    }
  },
  "headers": [
    {
      "source": "/api/cron/(.*)",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "no-cache, no-store, must-revalidate"
        }
      ]
    }
  ]
}